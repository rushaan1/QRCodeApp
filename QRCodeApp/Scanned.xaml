<Page x:Class="QRCodeApp.Scanned"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:QRCodeApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Background="Snow"
      Title="Scanned">

    <Viewbox>
        <Grid Width="800" Height="450">
            <Button Click="Back" Background="#FF9DBEEE" Foreground="Black" Width="100" Height="40" FontSize="22" Margin="10,10,690,400">Back</Button>

            <Image x:Name="scannedQR" Width="200" Height="200" Margin="299,26,301,224"></Image>
            <TextBox Background="#FF9DF168" IsReadOnly="True" BorderBrush="Transparent" TextWrapping="Wrap" x:Name="qrDetails" TextAlignment="Center" FontSize="24" FontFamily="Yu Gothic Light" Margin="196,226,196,49"></TextBox>
            <TextBlock x:Name="filename" TextAlignment="Center" Margin="82,413,76,10" FontFamily="Century Gothic" FontSize="16"></TextBlock>
            <!--<Button FontSize="25" Content="Previous" Click="Files" Background="#FF1CBD5E" Margin="320,148,329,250"/>-->
        </Grid>
    </Viewbox>
</Page>
