<Page x:Class="QRCodeApp.Scanned"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:QRCodeApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Background="Snow"
      Title="Scanned">

    <Viewbox>
        <Grid Width="800" Height="450">
            <Button Click="Back" Background="#FF9DBEEE" Foreground="Black" Width="100" Height="40" FontSize="22" Margin="10,10,690,400">Back</Button>

            <Image Width="200" Height="200" Margin="301,65,299,185"></Image>
            <TextBlock x:Name="qrDetails" TextAlignment="Center" FontSize="22" FontFamily="Yu Gothic Light" Margin="198,309,194,10"></TextBlock>
        </Grid>
    </Viewbox>
</Page>
